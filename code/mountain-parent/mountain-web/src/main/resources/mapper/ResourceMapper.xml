<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mountain.web.basic.mapper.ResourceMapper">

    <resultMap id="BaseResultMap" type="com.mountain.web.basic.pojo.po.Resource">
        <id property="id" column="id"/>
        <result property="parentId" column="parent_id"/>
        <result property="resourceName" column="resource_name"/>
        <result property="resourceCode" column="resource_code"/>
        <result property="resourceType" column="resource_type"/>
        <result property="url" column="url"/>
        <result property="requestType" column="request_type"/>
        <result property="authorizeType" column="authorize_type"/>
        <result property="sortNo" column="sort_no"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="creatorId" column="creator_id"/>
        <result property="modifierId" column="modifier_id"/>
        <result property="remark" column="remark"/>
        <result property="deleted" column="deleted"/>
    </resultMap>

</mapper>
